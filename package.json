{
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "auto-bind": "^5.0.1",
    "code-excerpt": "^4.0.0",
    "is-in-ci": "^0.1.0",
    "lodash": "^4.17.21",
    "ora": "^8.0.1",
    "react": "^18.2.0",
    "react-reconciler": "^0.29.0",
    "rxjs": "^7.8.1",
    "serialport": "^12.0.0",
    "stack-utils": "^2.0.6",
    "typescript": "^5.4.2",
    "yoga": "^0.0.20",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@types/ansi-escapes": "^4.0.0",
    "@types/auto-bind": "^2.1.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.8.8",
    "@types/react": "^18.2.55",
    "@types/react-reconciler": "^0.28.8",
    "@types/serialport": "^8.0.3",
    "@types/stack-utils": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "is-in-ci": "^0.1.0",
    "prettier": "^3.0.3",
    "vite-node": "^1.2.2",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^1.4.0",
    "vite": "^5.1.6"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE.md",
    "CONTRIBUTING.md"
  ],
  "name": "edison",
  "type": "module",
  "version": "0.1.35",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*.js"
  },
  "module": "./dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "description": "This package can control Arduino with TypeScript!",
  "scripts": {
    "build:esm": "tsc -p tsconfig.esm.json && node rename-to-esm.mjs",
    "build": "npm run build:esm",
    "dev": "vite",
    "serve": "vite preview",
    "lint": "npx eslint src/**/*.ts* ",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"./src/**/*.{ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edison-js/Edison.git"
  },
  "keywords": [
    "IoT",
    "Arduino",
    "TypeScript",
    "JavaScript",
    "Robotics",
    "edison",
    "edison.ts",
    "edison.js"
  ],
  "author": "aluta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edison-js/Edison/issues"
  },
  "homepage": "https://github.com/edison-js/Edison#readme"
}
